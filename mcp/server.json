        {
          "name": "SUPABASE_ANON_KEY",
          "description": "Supabase anonymous key (optional - defaults to public instance)",
          "isRequired": false,
          "isSecret": true,
          "placeholder": "your-anon-key"
        }
      ]
    }
  ],
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://riksdag-regering-ai.onrender.com/mcp"
    }
