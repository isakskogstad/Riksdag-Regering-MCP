  },
  "icons": [
    {
      "src": "https://mb.cision.com/Public/20518/3733746/9c8944283144c24f_800x800ar.png",
      "mimeType": "image/png",
      "sizes": ["800x800"]
    }
  ],
  "packages": [
    {
      "registryType": "npm",
      "identifier": "riksdag-regering-mcp",
      "version": "2.0.1",
